  "scripts": {
    "start": "gatsby develop",
    "develop": "gatsby develop -H 0.0.0.0 -p 8000",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "cy:open": "cypress open --config-file ./cypress.json",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test \"yarn develop\" http://localhost:8000 \"yarn cy:open\"",
    "test:e2e:ci": "start-server-and-test \"yarn develop\" http://localhost:8000 \"yarn cy:run\""
  },
