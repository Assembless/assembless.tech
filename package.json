{
  "name": "assembless.tech",
  "version": "1.0.0",
  "private": true,
  "author": "Assembless <support@assembless.tech>",
  "homepage": "https://assembless.tech/",
  "license": "MIT",
  "keywords": [
    "typescript"
  ],
  "scripts": {
    "start": "gatsby develop",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test \"yarn develop\" http://localhost:8000 \"yarn cy:open\"",
    "test:e2e:ci": "start-server-and-test \"yarn develop\" http://localhost:8000 \"yarn cy:run\"",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "dependencies": {
    "@fontsource/lato": "^4.2.2",
    "@fontsource/roboto-mono": "^4.2.2",
    "@material-ui/core": "^4.11.3",
